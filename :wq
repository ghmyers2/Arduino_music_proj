import serial
import time

ser = serial.Serial("/dev/ttyACM0", 115200)
time.sleep(1)

while True:
    print(decodeSerial(ser.readline()))

def decodeSerial(data):
    decodedStr = data.decode("utf-8")
    if decodedStr[0] != "(" or decodedStr[-1] != ")":
        return None
    return decodedStr[1:-1].split(",")
